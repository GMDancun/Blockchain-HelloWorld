
{
    "scripts": {
    "dfx_install": "DFX_VERSION=0.15.1 sh -ci \"$(curl -fsSL https://sdk.dfinity.org/install.sh)\"",
    "replica_start": "dfx start --background",
    "replica_stop": "dfx stop",
    "canister_deploy_local": "dfx deploy my_app",
    "canister_deploy_mainnet": "dfx deploy --network ic my_app",
    "canister_uninstall": "dfx canister uninstall-code my_app",
    "canister_call_get_message": "dfx canister call my_app getMessage",
    "canister_call_set_message": "dfx canister call my_app setMessage '(\"Hello world!\")'"
    },
    "dependencies": {
    "azle": "0.18.6",
    "uuid": "^9.0.0"
    },
    
    "engines": {
    "node" : "^12 || ^14 || ^16 || ^18"
    },
    
    "devDependencies": {
    "@types/uuid": "^9.0.1"
    }
}    