{
    "scripts": {
        "dfx_install": "DFX_VERSION=0.15.1 sh -ci \"$(curl -fsSL https://sdk.dfinity.org/install.sh)\"",
        "replica_start": "dfx start --background",
        "replica_stop": "dfx stop",
        "canister_deploy_local": "dfx deploy myapp",
        "canister_deploy_mainnet": "dfx deploy --network ic myapp",
        "canister_uninstall": "dfx canister uninstall-code myapp",
        "canister_call_get_message": "dfx canister call myapp getMessage",
        "canister_call_set_message": "dfx canister call myapp setMessage '(\"Hello world!\")'"
    },
    "dependencies": {
        "azle": "0.18.6",
        "uuid": "^9.0.0"
    },

    "engines": {
        "node": "^12 || ^14 || ^16 || ^18"
    },

    "devDependancies": {
        "@types/uuid": "^9.0.1"
    }
}

